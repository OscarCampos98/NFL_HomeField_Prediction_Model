)\n","\n","print(\"Continuous shape:\", df_cont.shape)\n","print(\"Noncontdf_noncont-continuous shape:\", df_noncont.shape)\n","\n","key_cols = [\"season\", \"week_num\", \"Date\", \"home_team\", \"away_team\"]\n","\n","def inspect_duplicates(df, name, key_cols):\n","    print(f\"\\n=== Inspecting {name} ===\")\n","    print(\"Total rows:\", len(df))\n","\n","    # Count how many rows per game key\n","    counts = (\n","        df\n","        .groupby(key_cols)\n","        .size()\n","        .reset_index(name=\"n_rows\")\n","        .sort_values(\"n_rows\", ascending=False)\n","    )\n","\n","    # How many games have duplicates?\n","    dup_games = counts[counts[\"n_rows\"] > 1]\n","    print(\"Number of distinct games with duplicates:\", len(dup_games))\n","\n","    if len(dup_games) > 0:\n","        print(\"\\nTop 10 duplicated games:\")\n","        print(dup_games.head(10))\n","\n","        # Take the worst-offender game and show the actual rows\n","        example = dup_games.iloc[0]\n","        mask = (df[key_cols] == example[key_cols]).all(axis=1)\n","        print(\"\\nExample duplicated game rows:\")\n","        print(df.loc[mask].head(10))\n","    else:\n","        print(\"No duplicated games found by key:\", key_cols)\n","\n","\n","inspect_duplicates(df_cont, \"games_elo_continuous\", key_cols)\n","inspect_duplicates(df_noncont,  \"games_elo_noncontinuous\", key_cols)\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"oPBghTOnKJ_b","executionInfo":{"status":"ok","timestamp":1768859476700,"user_tz":420,"elapsed":104,"user":{"displayName":"oscar campos","userId":"05775422317227794607"}},"outputId":"2b3d1016-7e92-4d42-9f70-dc67dd1c6935"},"execution_count":12,"outputs":[{"output_type":"stream","name":"stdout","text":["Continuous shape: (1869, 31)\n","Noncontdf_noncont-continuous shape: (1869, 31)\n","\n","=== Inspecting games_elo_continuous ===\n","Total rows: 1869\n","Number of distinct games with duplicates: 0\n","No duplicated games found by key: ['season', 'week_num', 'Date', 'home_team', 'away_team']\n","\n","=== Inspecting games_elo_noncontinuous ===\n","Total rows: 1869\n","Number of distinct games with duplicates: 0\n","No duplicated games found by key: ['season', 'week_num', 'Date', 'home_team', 'away_team']\n"]}]},{"cell_type":"code","source":["def check_dup_content(df, name, key_cols):\n","    print(f\"\\n=== Checking duplicate content for {name} ===\")\n","\n","    dup_mask = df.duplicated(subset=key_cols, keep=False)\n","    dup_df = df[dup_mask]\n","\n","    if dup_df.empty:\n","        print(\"No duplicate game keys; nothing to check.\")\n","        return\n","\n","    # Group by game key and see if all rows are identical in non-key columns\n","    non_key_cols = [c for c in df.columns if c not in key_cols]\n","\n","    def group_all_equal(g):\n","        # Compare each row to the first row in this group\n","        first = g[non_key_cols].iloc[0]\n","        return (g[non_key_cols] == first).all(axis=1).all()\n","\n","    eq_series = dup_df.groupby(key_cols).apply(group_all_equal)\n","    num_all_equal = eq_series.sum()\n","    num_groups = len(eq_series)\n","\n","    print(f\"Duplicated game groups: {num_groups}\")\n","    print(f\"Groups where ALL non-key columns are identical: {num_all_equal}\")\n","    print(f\"Groups where some non-key columns differ: {num_groups - num_all_equal}\")\n","\n","    if num_groups - num_all_equal > 0:\n","        print(\"\\nExample of a game with differing duplicate rows:\")\n","        example_key = eq_series[eq_series == False].index[0]\n","        mask = (df[key_cols] == example_key).all(axis=1)\n","        print(df.loc[mask])\n","\n","\n","check_dup_content(df_cont, \"games_elo_continuous\", key_cols)\n","check_dup_content(df_noncont,  \"games_elo_noncontinuous\", key_cols)\n"],"metadata":{"id":"mBwVBqcLK6U7","executionInfo":{"status":"ok","timestamp":1768859476702,"user_tz":420,"elapsed":11,"user":{"displayName":"oscar campos","userId":"0577542231