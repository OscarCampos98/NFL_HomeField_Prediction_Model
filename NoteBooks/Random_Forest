ed[\"season\"],\n","    \"week_num\": df_merged[\"week_num\"],\n","    \"Date\": df_merged[\"Date\"],\n","    \"result_for_team\": away_res,\n","    \"is_home\": 0\n","})\n","\n","long_wr_df = pd.concat([home_wr_long, away_wr_long], ignore_index=True)\n","long_wr_df = long_wr_df.sort_values([\"team\", \"season\", \"week_num\", \"Date\"])\n"],"metadata":{"id":"ckS7M6ISZNF6","executionInfo":{"status":"ok","timestamp":1768859482067,"user_tz":420,"elapsed":41,"user":{"displayName":"oscar campos","userId":"05775422317227794607"}}},"execution_count":30,"outputs":[]},{"cell_type":"code","source":["def add_winrate_and_streaks(group):\n","    # group is all rows for (team, season) already sorted by time\n","\n","    # Shift by 1 so the current game only uses PAST results\n","    res_prev = group[\"result_for_team\"].shift(1)\n","\n","    # Rolling win percentage over last 3 and 5 games\n","    group[\"win_pct_last3\"] = res_prev.rolling(3, min_periods=1).mean()\n","    group[\"win_pct_last5\"] = res_prev.rolling(5, min_periods=1).mean()\n","\n","    # Season-to-date win percentage BEFORE current game\n","    cum_wins = res_prev.fillna(0).cumsum()\n","    cum_games = res_prev.notna().cumsum()\n","    group[\"season_win_pct_to_date\"] = np.where(cum_games > 0, cum_wins / cum_games, np.nan)\n","\n","    # Win streak & losing streak lengths BEFORE current game\n","    win_streak = []\n","    lose_streak = []\n","    w_current = 0\n","    l_current = 0\n","\n","    for val in res_prev:\n","        if np.isnan(val):\n","            # Tie or no previous game: reset both\n","            w_current = 0\n","            l_current = 0\n","        elif val == 1:\n","            w_current += 1\n","            l_current = 0\n","        elif val == 0:\n","            l_current += 1\n","            w_current = 0\n","        else:\n","            # Just in case, reset on weird values\n","            w_current = 0\n","            l_current = 0\n","\n","        win_streak.append(w_current)\n","        lose_streak.append(l_current)\n","\n","    group[\"win_streak_len\"] = win_streak\n","    group[\"lose_streak_len\"] = lose_streak\n","\n","    return group\n","\n","# Apply per team & season\n","long_wr_df = long_wr_df.groupby([\"team\", \"season\"], group_keys=False).apply(add_winrate_and_streaks)\n"],"metadata":{"id":"vCP2AOrJf2jV","executionInfo":{"status":"ok","timestamp":1768859482589,"user_tz":420,"elapsed":520,"user":{"displayName":"oscar campos","userId":"05775422317227794607"}},"colab":{"base_uri":"https://localhost:8080/"},"outputId":"4c280440-f17b-478c-b52b-314eff3c12ef"},"execution_count":31,"outputs":[{"output_type":"stream","name":"stderr","text":["/tmp/ipython-input-1385936239.py:47: DeprecationWarning: DataFrameGroupBy.apply operated on the grouping columns. This behavior is deprecated, and in a future version of pandas the grouping columns will be excluded from the operation. Either pass `include_groups=False` to exclude the groupings or explicitly select the grouping columns after groupby to silence this warning.\n","  long_wr_df = long_wr_df.groupby([\"team\", \"season\"], group_keys=False).apply(add_winrate_and_streaks)\n"]}]},{"cell_type":"code","source":["\n","# Home-side win features\n","home_wr_feats = long_wr_df[long_wr_df[\"is_home\"] == 1].set_index(\"game_id\")[\n","    [\"win_pct_last3\", \"win_pct_last5\", \"season_win_pct_to_date\",\n","     \"win_streak_len\", \"lose_streak_len\"]\n","].add_prefix(\"home_\")\n","\n","# Away-side win features\n","away_wr_feats = long_wr_df[long_wr_df[\"is_home\"] == 0].set_index(\"game_id\")[\n","    [\"win_pct_last3\", \"win_pct_last5\", \"season_win_pct_to_date\",\n","     \"win_streak_len\", \"lose_streak_len\"]\n","].add_prefix(\"away_\")\n","\n","df_merged = df_merged.set_index(\"game_id\")\n","df_merged = df_merged.join(home_wr_feats).join(away_wr_feats)\n","df_merged = df_merged.reset_index(drop=True)\n","\n","# Difference features (home - away)\n","df_merged[\"win_pct_last3_diff\"] = df_merged[\"home_win_pct_last3\"] - df_merged[\"away_win_pct_last3\"]\n","df_merged[\"win_pct_last5_diff\"] = df_merged[\"home_win_pct_last5\"] - df_merged[\"away_win_pct_last5\"]\n","df_merged[\"season_win_pct_diff\"] = df_merged[\"home_season_win_pct_to_date\"] - df_merged[\"away_season_win_pct_to_date\"]\n","df_merged[\"win_streak_len_diff\"] = df_merged[\"home_win_streak_len\"] - df_merged[\"away_win_streak_len\"]\n","df_merged[\"lose_streak_len_diff\"] = df_merged[\"home_lose_streak_len\"] - df_merged[\"away_lose_streak_len\"]\n"],"metadata":{"id":"7K6kv_ySgUPw","executionInfo":{"status":"ok","timestamp":1768859482615,"user_tz":420,"elapsed":24,"user":{"displayName":"oscar campos","userId":"05775422317227794607"}}},"execution_count":32,"outputs":[]},{"cell_type":"code","source":["# Drop ties again for the target\n","df_model = df_merged[df_merged[\"home_win\"] != 0.5].copy()\n","\n","base_feature_cols = [\n","    \"pre_elo_home_cont\",\n","    \"pre_elo_away_cont\",\n","    \"elo_diff_cont\",\n","    \"pre_elo_home_noncont\",\n","    \"pre_elo_away_noncont\",\n","    \"elo_diff_noncont\",\n","    \"expected_home_win_prob\",\n","    \"home_field_advantage_pts\",\n","    \"is_international\",\n","]\n","\n","rolling_elo_feature_cols = [\n","    \"elo_diff_cont_roll3\",\n","    \"elo_diff_cont_roll5\",\n","    \"elo_diff_noncont_roll3\",\n","    \"elo_diff_noncont_roll5\",\n","]\n","\n","mov_feature_cols = [\n","    \"mov_diff_roll3\",\n","    \"mov_diff_roll5\",\n","]\n","\n","winrate_feature_cols = [\n","    \"win_pct_last3_diff\",\n","    \"win_pct_last5_diff\",\n","    \"season_win_pct_diff\",\n","    \"win_streak_len_diff\",\n","    \"lose_streak_len_diff\",\n","]\n","\n","feature_cols = (\n","    base_feature_cols\n","    + rolling_elo_feature_cols\n","    + mov_feature_cols\n","    + winrate_feature_cols\n",")\n","\n","X = df_model[feature_cols]\n","y = df_model[\"home_win\"].astype(int)\n","print(\"Total features:\", len(feature_cols))\n"],"metadata":{"id":"V