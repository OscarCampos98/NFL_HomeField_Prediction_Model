(season, team):\n","  key = (season,team)\n","  if key not in team_state:\n","    raise KeyError(f\"Missing team state for {key}\")\n","  return team_state[key]\n","\n","df_future[\"pre_elo_home_cont\"] = df_future.apply( lambda r: get_team_state(r[\"season\"], r[\"home_team_norm\"])[\"post_elo_cont\"], axis=1)\n","df_future[\"pre_elo_away_cont\"] = df_future.apply( lambda r: get_team_state(r[\"season\"], r[\"away_team_norm\"])[\"post_elo_cont\"], axis=1)\n","\n","df_future[\"elo_diff_cont\"] = df_future[\"pre_elo_home_cont\"] - df_future[\"pre_elo_away_cont\"]\n"],"metadata":{"id":"ocY6cr2wvT42","executionInfo":{"status":"ok","timestamp":1768859496340,"user_tz":420,"elapsed":49,"user":{"displayName":"oscar campos","userId":"05775422317227794607"}}},"execution_count":51,"outputs":[]},{"cell_type":"code","source":["#compute the expected_home_win_prob\n","ELO_SCALE = 400\n","elo_with_hfa = df_future[\"elo_diff_cont\"] + df_future[\"home_field_advantage_pts\"]\n","df_future[\"expected_home_win_prob\"] = 1.0 / (1.0 + 10 ** (-(elo_with_hfa / ELO_SCALE)))\n","\n","df_future[\"expected_home_win_prob\"].describe()\n","df_future[[\"home_team_norm\", \"away_team_norm\", \"elo_diff_cont\", \"home_field_advantage_pts\", \"expected_home_win_prob\"]].head(10)"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":363},"id":"PNI851mA4WJD","executionInfo":{"status":"ok","timestamp":1768859496441,"user_tz":420,"elapsed":99,"user":{"displayName":"oscar campos","userId":"05775422317227794607"}},"outputId":"1015f005-628d-44ad-cd97-8d20ed70eeea"},"execution_count":52,"outputs":[{"output_type":"execute_result","data":{"text/plain":["          home_team_norm         away_team_norm  elo_diff_cont  \\\n","6          Detroit Lions         Dallas Cowboys     231.881403   \n","7   Tampa Bay Buccaneers     New Orleans Saints     236.435299   \n","8          New York Jets         Miami Dolphins     -92.209888   \n","9       Cleveland Browns       Tennessee Titans     117.147471   \