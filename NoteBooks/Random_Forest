ut (just in case file has older data)\n","df = df[df['season'].between(2019,2025)].copy()\n","\n","print(\"Data after filtering seasons & ties\", df.shape)\n","\n","#define feature columns for phase 1\n","feature_cols = [\n","    'pre_elo_home',\n","    'pre_elo_away',\n","    'pre_elo_home',\n","    'home_field_advantage_pts',\n","    'is_international',\n","    'expected_home_win_prob',\n","    'season',\n","    'week_num'\n","]\n","\n","#Quick sanity check: all columns exist\n","missing = [col for col in feature_cols if col not in df.columns]\n","if missing:\n","  raise ValueError(f\"Missing expected columns in df: {missing}\")\n","\n","#Handle any missing values\n","print(\"Missing Values per column\")\n","print(df[feature_cols + ['home_win']].isna().sum())\n","\n","#for now lets drop the NA rows in feature/target\n","df = df.dropna(subset=feature_cols + ['home_win']).copy()\n","print(\"Data after dropping missing values\", df.shape)\n","\n","\n","#Time-based splt: train(2019-2023), valid(2023), test(2024)\n","train_seasons = [2019,2020,2021,2022,2023]\n","valid_seasons = [2024]\n","test_seasons = [2025]\n","df_train = df[df['season'].isin(train_seasons)].copy()\n","df_valid = df[df['season'].isin(valid_seasons)].copy()\n","df_test = df[df['season'].isin(test_seasons)].copy()\n","\n","print(\"Train shape\", df_train.shape)\n","print(\"Valid shape\", df_valid.shape)\n","print(\"Train test\", df_test.shape)\n","\n","# X and Y for each splt\n","x_train = df_train[feature_cols].values\n","y_train = df_train['home_win'].values\n","\n","x_valid = df_valid[feature_cols].values\n","y_valid = df_valid['home_win'].values\n","\n","x_test = df_test[feature_cols].values\n","y_test = df_test['home_win'].values\n","\n","print(\"x_train:\", x_train.shape, 'y_train:',y_train.shape)\n","print(\"x_valid:\", x_valid.shape, 'y_valid:',y_valid.shape)\n","print(\"x_test:\", x_test.shape, 'y_test:',y_test.shape)\n"],"metadata":{"id":"doNtHSprGOLj","colab":{"base_uri":"https://localhost